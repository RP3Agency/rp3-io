{
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"  
    },
    {
      "type": "package",
      "package": {
        "name": "wordpress",
        "type": "webroot",
        "version": "4.1.1",
        "dist": {
          "type": "zip",
          "url": "https://github.com/WordPress/WordPress/archive/4.1.1.zip"
        },
        "require" : {
          "fancyguy/webroot-installer": "1.0.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "advanced-custom-fields-pro",
        "type": "wordpress-plugin",
        "version": "5.1.9.1",
        "dist": {
          "type": "zip",
          "url": "http://composer.rp3dev.com/advanced-custom-fields-pro.zip"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wp-migrate-db-pro",
        "type": "wordpress-plugin",
        "version": "1.4.6",
        "dist": {
          "type": "zip",
          "url": "http://composer.rp3dev.com/wp-migrate-db-pro.zip"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wp-migrate-db-pro-media-files",
        "type": "wordpress-plugin",
        "version": "1.3.1",
        "dist": {
          "type": "zip",
          "url": "http://composer.rp3dev.com/wp-migrate-db-pro-media-files.zip"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wp-migrate-db-pro-cli",
        "type": "wordpress-plugin",
        "version": "1.1",
        "dist": {
          "type": "zip",
          "url": "http://composer.rp3dev.com/wp-migrate-db-pro-cli.zip"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "wordpress": "4.1.1",
    "fancyguy/webroot-installer": "1.0.0",
    "advanced-custom-fields-pro": "*",
    "wp-migrate-db-pro": "*",
    "wp-migrate-db-pro-media-files": "*",
    "wp-migrate-db-pro-cli": "*",
    "wpackagist-plugin/add-from-server":"*",
    "wpackagist-plugin/admin-post-navigation":"*",
    "wpackagist-plugin/akismet":"*",
    "wpackagist-plugin/black-studio-tinymce-widget":"*",
    "wpackagist-plugin/co-authors-plus":"*",
    "wpackagist-plugin/contact-form-7":"*",
    "wpackagist-plugin/custom-menu-wizard":"*",
    "wpackagist-plugin/custom-tag-widget":"*",
    "wpackagist-plugin/debug-bar":"*",
    "wpackagist-plugin/disable-comments":"*",
    "wpackagist-plugin/flipboard-magazine-widget":"*",
    "wpackagist-plugin/fv-top-level-cats":"*",
    "wpackagist-plugin/google-analytics-for-wordpress":"*",
    "wpackagist-plugin/jetpack":"*",
    "wpackagist-plugin/list-category-posts":"*",
    "wpackagist-plugin/mailchimp-for-wp":"*",
    "wpackagist-plugin/markdown-on-save":"*",
    "wpackagist-plugin/public-post-preview":"*",
    "wpackagist-plugin/really-simple-twitter-feed-widget":"*",
    "wpackagist-plugin/regenerate-thumbnails":"*",
    "wpackagist-plugin/redirection":"*",
    "wpackagist-plugin/show-template":"*",
    "wpackagist-plugin/simple-page-ordering":"*",
    "wpackagist-plugin/tc-custom-taxonomy-filter":"*",
    "wpackagist-plugin/theme-check":"*",
    "wpackagist-plugin/widget-logic":"*",
    "wpackagist-plugin/wordpress-importer":"*",
    "wpackagist-plugin/worker":"*",
    "wpackagist-plugin/wp-maintenance-mode":"*",
    "wpackagist-plugin/wp-siren-recent-posts-by-category":"*",
    "wpackagist-plugin/wpclef":"*"
  },
  "extra": {
    "webroot-dir": "_wp",
    "webroot-package": "wordpress",
    "installer-paths": {
      "wp-content/plugins/{$name}": ["type:wordpress-plugin"]
    }
  }
}
